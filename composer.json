    "keywords": [
      "pyrocms",
      "laravel",
        "addon",
        "theme"
    ],
    "homepage": "http://websemantics.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "websemantics",
            "email": "suport@websemantics.com",
            "homepage": "http://websemantics.com/",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "support@websemantics.com"
